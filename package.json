{
  "name": "@uni-helper/uni-use",
  "version": "0.13.1",
  "description": "uni-app (vue3) 组合式工具集",
  "keywords": [
    "uni-app",
    "uniapp",
    "uni",
    "use",
    "composition",
    "composable"
  ],
  "homepage": "https://github.com/uni-helper/uni-use#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-use/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-use.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "check:deps": "taze",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "unbuild --stub",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\" \"pnpm:lint:publint\"",
    "lint:eslint": "eslint ./src --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:publint": "publint",
    "prepare": "is-ci || simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm install && pnpm run lint && bumpp"
  },
  "dependencies": {
    "@dcloudio/types": "^3.3.3",
    "@dcloudio/uni-app": "^3.0.0-alpha-3080220230428001",
    "@vueuse/core": "^9.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/prompt": "^17.6.3",
    "@modyqyw/fabric": "^8.0.0",
    "@modyqyw/utils": "^6.0.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "bumpp": "^9.1.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.0.1",
    "eslint": "8.39.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.2",
    "markdownlint-cli": "^0.34.0",
    "prettier": "^2.8.8",
    "publint": "^0.1.12",
    "simple-git-hooks": "^2.8.1",
    "sort-package-json": "^2.4.1",
    "taze": "^0.10.1",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "@vueuse/core": "^9.0.0",
    "typescript": "^4.5.0 || ^5.0.0",
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.5.1",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
