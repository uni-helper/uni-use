{
  "name": "@uni-helper/uni-use",
  "version": "0.17.1",
  "description": "uni-app (vue3) 组合式工具集",
  "keywords": [
    "uni-app",
    "uniapp",
    "uni",
    "use",
    "composition",
    "composable"
  ],
  "homepage": "https://github.com/uni-helper/uni-use#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-use/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-use.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "check:deps": "taze -f",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "unbuild --stub",
    "lint": "conc \"pnpm:lint:eslint\" \"pnpm:lint:markdownlint\" \"pnpm:lint:publint\"",
    "lint:eslint": "eslint ./src --fix --cache --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:publint": "publint",
    "prepare": "is-ci || simple-git-hooks",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm install && pnpm run lint && bumpp"
  },
  "dependencies": {
    "@dcloudio/types": "^3.4.3",
    "@dcloudio/uni-app": "^3.0.0-3090620231104002",
    "@vueuse/core": "^9.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@commitlint/prompt": "^18.4.1",
    "@modyqyw/fabric": "^9.0.6",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "bumpp": "^9.2.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.1.0",
    "markdownlint-cli": "^0.38.0",
    "prettier": "^3.0.3",
    "publint": "^0.2.5",
    "simple-git-hooks": "^2.9.0",
    "sort-package-json": "^2.6.0",
    "taze": "^0.13.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vue": "^3.3.8"
  },
  "peerDependencies": {
    "@vueuse/core": "^9.0.0",
    "typescript": "^4.5.0 || ^5.0.0",
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
