{
  "name": "@uni-helper/uni-use",
  "version": "0.12.0",
  "description": "uni-app (vue3) 组合式工具集",
  "keywords": [
    "uni-app",
    "uniapp",
    "uni",
    "use",
    "composition",
    "composable"
  ],
  "homepage": "https://github.com/uni-helper/uni-use#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-use/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-use.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "commit": "git-cz",
    "dev": "unbuild --stub",
    "lint": "run-p lint:eslint lint:markdownlint check:types",
    "lint:eslint": "eslint ./src --fix --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm install && pnpm run lint && bumpp",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@dcloudio/types": "^3.2.11",
    "@dcloudio/uni-app": "^3.0.0-alpha-3061620221230002",
    "@vueuse/core": "^9.12.0",
    "immer": "^9.0.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@commitlint/prompt": "^17.4.3",
    "@modyqyw/fabric": "^7.0.3",
    "@modyqyw/utils": "^4.1.0",
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "bumpp": "^8.2.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.2",
    "markdownlint-cli": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.3",
    "typescript": "^5.0.0",
    "unbuild": "^1.1.1",
    "vue": "^3.2.47"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@7.27.0",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
